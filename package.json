{
  "name": "walrus-learning",
  "version": "1.0.0",
  "description": "A comprehensive learning and testing repository for Sui blockchain's Walrus decentralized storage system",
  "main": "index.js",
  "scripts": {
    "start": "echo 'Welcome to Walrus Learning Repository! See README.md for getting started.'",
    "test": "npm run test:typescript && npm run test:python && npm run test:cli",
    "test:typescript": "cd examples/typescript && npm test",
    "test:python": "cd examples/python && python -m pytest",
    "test:cli": "cd examples/cli && ./run-tests.sh",
    "dev": "cd examples/web-app && npm run dev",
    "build": "cd examples/web-app && npm run build",
    "setup": "./scripts/setup.sh",
    "clean": "rm -rf node_modules && rm -rf examples/*/node_modules"
  },
  "keywords": [
    "walrus",
    "sui",
    "blockchain",
    "storage",
    "decentralized",
    "blobs",
    "web3"
  ],
  "author": "Walrus Learning Community",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.0.0",
    "@mysten/walrus": "^1.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/walrus-learning.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/walrus-learning/issues"
  },
  "homepage": "https://github.com/your-username/walrus-learning#readme"
}